<br>                                             
<span style="font-size:<%=@internal.attribute7size*5%>px;"><%= @attributes[7] %> </span>
<span style="font-size:<%=@internal.attribute8size*5%>px;"><%= @attributes[8] %> </span>
<span style="font-size:<%=@internal.attribute9size*5%>px;"><%= @attributes[9] %> </span>
</div>
<% else %>
